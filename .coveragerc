[run]
source = 
    peliculas
    users
omit = 
    */migrations/*
    */tests/*
    */__init__.py
    manage.py
    *wsgi.py
    *asgi.py
    */admin.py
    */apps.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    raise NotImplementedError
    if settings.DEBUG
    if __name__ == .__main__.: